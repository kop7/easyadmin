{
  "type": "project",
  "license": "proprietary",
  "require": {
    "php": ">=8.2",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "symfony/console": "^7.1",
    "symfony/dotenv": "^7.1",
    "symfony/flex": "^2",
    "symfony/framework-bundle": "^7.1",
    "symfony/runtime": "^7.1",
    "symfony/security-bundle": "^7.1",
    "symfony/twig-bundle": "^7.1",
    "symfony/yaml": "^7.1",
    "twig/twig": "^3.0",
    "doctrine/orm": "^3.2",
    "doctrine/doctrine-bundle": "^2.13",
    "doctrine/doctrine-migrations-bundle": "^3.4",
    "symfony/orm-pack": "^2.4",
    "symfony/form": "^7.1",
    "symfony/validator": "^7.1",
    "easycorp/easyadmin-bundle": "^4.10"
  },
  "require-dev": {
    "symfony/maker-bundle": "^1.61",
    "symfony/profiler-pack": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "extra": {
    "symfony": {
      "allow-contrib": true,
      "require": "^7.1"
    }
  },
  "config": {
    "allow-plugins": {
      "symfony/flex": true,
      "symfony/runtime": true
    },
    "sort-packages": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
